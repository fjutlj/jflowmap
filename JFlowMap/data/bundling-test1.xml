<?xml version="1.0" encoding="UTF-8"?>
<graphml xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns="http://graphml.graphdrawing.org/xmlns"
         xsi:schemaLocation="http://graphml.graphdrawing.org/xmlns http://graphml.graphdrawing.org/xmlns/1.0/graphml.xsd">

   <key id="x" for="node" attr.name="x" attr.type="double"/>
   <key id="y" for="node" attr.name="y" attr.type="double"/>
   <key id="name" for="node" attr.name="name" attr.type="string"/>
   <key id="data" for="edge" attr.name="data" attr.type="double"/>

   <graph edgedefault="directed">
      <node id="A">
         <data key="x">0</data>
         <data key="y">0</data>
         <data key="name">A</data>
      </node>
      <node id="B">
         <data key="x">100</data>
         <data key="y">0</data>
         <data key="name">B</data>
      </node>
      <node id="A1">
         <data key="x">5</data>
         <data key="y">2</data>
         <data key="name">A1</data>
      </node>
      <node id="B1">
         <data key="x">58</data>
         <data key="y">-2</data>
         <data key="name">B1</data>
      </node>
      <node id="A2">
         <data key="x">15</data>
         <data key="y">5</data>
         <data key="name">A2</data>
      </node>
      <node id="B2">
         <data key="x">52</data>
         <data key="y">-10</data>
         <data key="name">B2</data>
      </node>
      <node id="A3">
         <data key="x">5</data>
         <data key="y">-5</data>
         <data key="name">A3</data>
      </node>
      <node id="B3">
         <data key="x">80</data>
         <data key="y">0</data>
         <data key="name">B3</data>
      </node>


      <!-- 
      <edge source="A" target="B" id="1">
         <data key="data">3</data>
      </edge>
      <edge source="A2" target="B2" id="1">
         <data key="data">5</data>
      </edge>
       -->
      <edge source="A1" target="B1" id="1">
         <data key="data">4</data>
      </edge>
      <edge source="A3" target="B3" id="1">
         <data key="data">4</data>
      </edge>

   </graph>
</graphml>
